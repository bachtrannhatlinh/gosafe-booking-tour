{
  "name": "@vilandtravel/web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:clean": "npm run clean && next build",
    "build:production": "cross-env NODE_ENV=production NEXT_TELEMETRY_DISABLED=1 next build",
    "build:fast": "cross-env SKIP_ENV_VALIDATION=1 NEXT_TELEMETRY_DISABLED=1 GENERATE_SOURCEMAP=false next build",
    "build:minimal": "cross-env SKIP_ENV_VALIDATION=1 NEXT_TELEMETRY_DISABLED=1 GENERATE_SOURCEMAP=false next build --no-lint",
    "build:debug": "next build --experimental-debug-memory-usage",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "clean": "rimraf .next out dist node_modules/.cache",
    "clean:hard": "rimraf .next out dist node_modules/.cache tsconfig.tsbuildinfo",
    "analyze": "cross-env ANALYZE=true next build",
    "build:memory": "cross-env NODE_OPTIONS=\"--max-old-space-size=8192\" next build"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.0",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-navigation-menu": "^1.2.13",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@tanstack/react-query": "^5.0.0",
    "axios": "^1.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.0.0",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.292.0",
    "next": "14.0.0",
    "react": "^18",
    "react-day-picker": "^9.8.1",
    "react-dom": "^18",
    "react-hook-form": "^7.47.0",
    "react-toastify": "^11.0.5",
    "tailwind-merge": "^2.0.0",
    "zod": "^3.22.0",
    "zustand": "^4.4.0"
  },
  "devDependencies": {
    "@netlify/plugin-nextjs": "^5.12.0",
    "@next/bundle-analyzer": "^15.4.6",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10",
    "cross-env": "^10.0.0",
    "eslint": "^8",
    "eslint-config-next": "14.0.0",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  }
}
